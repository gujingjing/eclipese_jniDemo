#include<stdio.h>
#include<stdlib.h>
#include<jni.h>
/**
 *jstring：返回值，String -jstring -String
 *方法名：
 *Java_全类名(把.变成_)_方法名
 *JNIEnv* env：const struct JNINativeInterface**
 *  (*env).等价于 env->
    (**env).等价  (*env)->
 *
 *jobj：谁调用这个方法就是谁的实例，当前是MainActivity.this
 *
 */

jstring Java_com_itheima_jnihelloworld_MainActivity_say_11_1333_1111Hello(JNIEnv* env,jobject jobj){
	//jstring     (*NewStringUTF)(JNIEnv*, const char*);
	char* text = "I am from C!!! use (**env).";
	return (**env).NewStringUTF(env,text);
}
